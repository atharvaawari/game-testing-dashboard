                      </div>
                    </div>
                    <!-- update targets pop -->
                  </td>
                  <td>
                   <button onclick="deleteTask('<%=gamesTasks[a].id%>','weekly')" class="pop-up-btn btn  btn-secondary f-icon"> üóëÔ∏è </button>
